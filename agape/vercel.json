{
  "version": 2,
  "builds": [
    {
      "src": "agape/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "destination": "/agape/wsgi.py"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/agape/wsgi.py" }]
}
