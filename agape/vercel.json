{
  "builds": [
    {
      "src": "agape/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.8" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "agape/wsgi.py"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    }
  ]
}
